      <OMA>
         <OMS cd="meta" name="CDName"/>
         <OMSTR>units_imperial1</OMSTR>
      </OMA>
      <OMA>
         <OMS cd="meta" name="CDURL"/>
         <OMSTR>http://www.openmath.org/cd/units_imperial1.ocd</OMSTR>
      </OMA>
