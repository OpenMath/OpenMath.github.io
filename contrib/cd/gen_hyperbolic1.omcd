                     <OMV name="z"/>
                     <OMS cd="setname1" name="C"/>
                  </OMA>
                  <OMA>
                     <OMS cd="relation1" name="eq"/>
                     <OMA>
                        <OMA>
                           <OMS cd="gen_hyperbolic1" name="generalised_hyperbolic"/>
                           <OMI> 1 </OMI>
                           <OMI> 1 </OMI>
                           <OMI> 0 </OMI>
                        </OMA>
                        <OMV name="z"/>
                     </OMA>
                     <OMA>
                        <OMS cd="transc1" name="exp"/>
                        <OMV name="z"/>
                     </OMA>
                  </OMA>
               </OMA>
            </OMBIND>
         </OMA>
         <OMA>
            <OMS cd="meta" name="CMP"/>
