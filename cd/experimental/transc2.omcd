                <OMV name="y"/>
              </OMA>
            </OMA>
            <OMS name="zero" cd="alg1"/>
          </OMA>
        </OMA>
      </OMA>
      <OMA>
        <OMS cd="meta" name="CMP"/>
        <OMSTR>$Re(y)=0 and Re(x)&gt;0 \implies Re(arctan(y,x))=0$.</OMSTR>
      </OMA>
      <OMA>
        <OMS cd="meta" name="FMP"/>
        <OMA>
          <OMS name="implies" cd="logic1"/>
          <OMA>
            <OMS name="and" cd="logic1"/>
            <OMA>
