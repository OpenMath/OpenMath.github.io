            <OMS name="lt" cd="relation1"/>
            <OMA>
              <OMS name="real" cd="complex1"/>
              <OMA>
                <OMS name="arctan" cd="transc2"/>
                <OMV name="x"/>
                <OMV name="y"/>
              </OMA>
            </OMA>
            <OMS name="zero" cd="alg1"/>
          </OMA>
        </OMA>
      </OMA>
      <OMA>
        <OMS cd="meta" name="CMP"/>
        <OMSTR>$Re(y)=0 and Re(x) &lt; 0 \implies Re(arctan(y,x))=\pi$.</OMSTR>
      </OMA>
      <OMA>
        <OMS cd="meta" name="FMP"/>
