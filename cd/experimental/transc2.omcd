          <OMA>
            <OMS name="gt" cd="relation1"/>
            <OMA>
              <OMS name="real" cd="complex1"/>
              <OMV name="y"/>
            </OMA>
            <OMS name="zero" cd="alg1"/>
          </OMA>
          <OMA>
            <OMS name="gt" cd="relation1"/>
            <OMA>
              <OMS name="real" cd="complex1"/>
              <OMA>
                <OMS name="arctan" cd="transc2"/>
                <OMV name="x"/>
                <OMV name="y"/>
              </OMA>
            </OMA>
            <OMS name="zero" cd="alg1"/>
          </OMA>
        </OMA>
      </OMA>
      <OMA>
        <OMS cd="meta" name="CMP"/>
        <OMSTR>$Re(y) &lt; 0 \implies Re(arctan(y,x)) &lt; 0$.</OMSTR>
      </OMA>
      <OMA>
