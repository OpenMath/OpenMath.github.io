              <OMA>
                <OMS name="real" cd="complex1"/>
                <OMV name="y"/>
              </OMA>
              <OMS name="zero" cd="alg1"/>
            </OMA>
            <OMA>
              <OMS name="gt" cd="relation1"/>
              <OMA>
                <OMS name="real" cd="complex1"/>
                <OMV name="x"/>
              </OMA>
              <OMS name="zero" cd="alg1"/>
            </OMA>
          </OMA>
