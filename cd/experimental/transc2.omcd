          </OMA>
          <OMA>
            <OMS cd="set1" name="in"/>
            <OMA>
              <OMS cd="transc2" name="unwind"/>
              <OMV name="z"/>
            </OMA>
            <OMS cd="setname1" name="Z"/>
          </OMA>
        </OMA>
      </OMA>
      <OMA>
        <OMS cd="meta" name="CDComment"/>
        <OMSTR>\arcsin z = \arctan\frac z{\sqrt{1-z^2}} +\pi\K(-\ln(1+z))-\pi\K(-\ln(1-z)).</OMSTR>
      </OMA>
