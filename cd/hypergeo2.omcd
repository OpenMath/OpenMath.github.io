                                       <OMV name="v"/>
                                       <OMV name="z"/>
                                    </OMA>
                                 </OMBIND>
                              </OMA>
                           </OMBIND>
                        </OMA>
                     </OMA>
                     <OMA>
                        <OMS cd="arith1" name="times"/>
                        <OMV name="z"/>
                        <OMA>
                           <OMS cd="calculus1" name="diff"/>
                           <OMBIND>
                              <OMS cd="fns1" name="lambda"/>
                              <OMBVAR>
                                 <OMV name="z"/>
                              </OMBVAR>
                              <OMA>
