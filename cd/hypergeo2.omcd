                           <OMS cd="calculus1" name="diff"/>
                           <OMBIND>
                              <OMS cd="fns1" name="lambda"/>
                              <OMBVAR>
                                 <OMV name="z"/>
                              </OMBVAR>
                              <OMA>
                                 <OMS cd="hypergeo2" name="besselY"/>
