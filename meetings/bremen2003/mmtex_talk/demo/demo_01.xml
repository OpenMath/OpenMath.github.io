      <subsection no="2" id="DEFAULT_ID_9">
        <headline>Verbatim text</headline>
        <par>
          Some verbatim Text:
        </par>
<verbatim class="code"> This is verbatim text. Commands, 
 
   \section{Name}
     \foo[bar]{an argment}

 should not be interpreted.</verbatim>
        <par>
          Inline verbatim text: <verb>\foo{bar}</verb> .
        </par>
      </subsection>
